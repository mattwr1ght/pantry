<!DOCTYPE html>
<html>
  <head>
    <title>Pantry</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "layouts/header" %>

    <div class="content">
      <%= yield %>
    </div>

  </body>


  <footer>
     <%= render "layouts/footer" %>
  </footer>

</html>
